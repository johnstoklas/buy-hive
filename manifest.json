{
  "manifest_version": 3,
  "name": "Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "./popup.html",
    "default_icon": "public/images/spider_man.png"
  },
  "background": { 
    "service_worker": "background.js"  
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webNavigation",
    "identity",
    "http://127.0.0.1:8000/"
  ],
  "host_permissions":["http://127.0.0.1:8000/"],
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./dist/popup.bundle.js"],
      "run_at": "document_end"
    }
  ]
}
