{
    "manifest_version": 3,
    "name": "Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "images/spider_man.png"
    },
    "permissions": ["storage", "activeTab", "scripting", "webNavigation"],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ]
}